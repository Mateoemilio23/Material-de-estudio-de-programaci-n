//Generar valores aleatorios 
#include<stdio.h>
#include<stdlib.h>
#include<time.h>
#include<math.h>
int main()
{
	int i,n,cont=0,acu2;
	float media,acu=0,varianza;
	srand(time(NULL));
	printf("Ingrese el tama침o del vector:");
	scanf("%d",&n);
	//Definici칩n del vector
	int vec[n];
	
	//Insertar datos en el vector
	printf("INSERTAR AUTOMATICAMENTE DATOS\n");
	for(i=0; i<n; i++)
	{
		vec[i]=rand()%11;
		acu=acu+vec[i];
	}
	
	//Imprimir Vector
	printf("IMPRIMIR DATOS\n");
	for(i=0; i<n; i++)
	{
		printf("%d\t",vec[i]);
	}
	
	//C치lculo de la Media
	media=acu/n;
	printf("\nLa media es:%f",media);
	
	//C치lculo de la Varianza
	for(i=0; i<n; i++)
	{
		acu2=pow((vec[i]-media),2);
	}
	varianza=acu2/n;
	printf("\nLa varianza es:%f",varianza);
	getch();
	return 0;
}
