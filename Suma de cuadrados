#include <stdio.h>
int main() 
{
    int N;
    int suma = 0;
    printf("Ingrese el numero entero N: ");
    scanf("%d", &N);
    if (N < 1) {
        printf("N debe ser un numero entero positivo.\n");
        return 0;
    }
    for (int i = 1; i <= N; i++) {
        suma += i * i;
    }
    printf("La suma de los cuadrados de los %d primeros numeros naturales es: %d\n", N, suma);
    getch();
    return 0;
}
