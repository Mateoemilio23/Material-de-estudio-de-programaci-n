#include <stdio.h>
int main() {
    int M, N;
    int sumatoria = 0;
    printf("Ingrese el numero entero M: ");
    scanf("%d", &M);
    printf("Ingrese el numero entero N: ");
    scanf("%d", &N);
    if (M > N) {
        printf("M debe ser menor o igual que N.\n");
        return 0;
    }
    for (int i = M; i <= N; i++) {
        sumatoria += i;
    }
    printf("La sumatoria de los numeros enteros de %d a %d es: %d\n", M, N, sumatoria);
    getch();
    return 0;
}
