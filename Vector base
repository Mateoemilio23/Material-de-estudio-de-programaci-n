#include<stdio.h>
int main()
{
	int N,i, acu=0;
	printf("Ingrese el tamano del vector:");
	scanf("%d",&N);
	
	//Definici√≥n del Vector
	int edades[N];
	
	//Ingresar datos en el vector
	for(i=0;i<N;i++)
	{
		printf("Ingrese la edad(%d):",i);
		scanf("%d",&edades[i]);
		acu=acu+edades[i];
	}
	
	//Imprimir los elementos del vector
	for(i=0;i<N;i++)
	{
		printf("%d\t",edades[i]);
	}
	printf("La suma es: %d",acu);
	getch();
	return 0;
}
