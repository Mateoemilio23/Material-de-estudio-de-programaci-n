#include <stdio.h> //Biblioteca  para operaciones de entrada y salida
int main() //Función principal del programa
{
    int numero, i; // Variables para almacenar el número ingresado por el usuario y el contador del bucle
    printf("Ingrese un numero: ");
    scanf("%d", &numero); // Solicitar al usuario que ingrese un número y almacenarlo en la variable 'numero'

    for (i = 1; i <= 10; i++) // Estructura de control de repetición 'for', se ejecutará 10 veces (desde 1 hasta 10)
	{ 
        printf("%d x %d = %d\n", numero, i, numero * i); // Imprimir la multiplicación del número ingresado por el valor actual de 'i'
    }
    getch();
    return 0;
}
