#include <stdio.h>
int main() 
{
    int numero;
    printf("Ingrese un numero entero positivo: ");
    scanf("%d", &numero);
    if (numero < 1) {
        printf("El numero debe ser positivo.\n");
        return 0;
    }
    printf("Tabla de multiplicar del numero %d:\n", numero);
    for (int i = 1; i <= 20; i++) {
        printf("%d x %d = %d\n", numero, i, numero * i);
    }
    getch();
    return 0;
}
